{
    "name": "kdkaushik/angel-one-api-php",
    "description": "PHP client library for Angel One (Angel Broking) API integration",
    "type": "library",
    "keywords": ["angel-one", "angel-broking", "trading", "api", "php", "stock-market"],
    "homepage": "https://github.com/kdkaushik/angel-one-api-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Keshav D Kaushik",
            "email": "kdkaushik@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "AngelOneAPI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AngelOneAPI\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}